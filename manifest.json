{
  "name": "TheLastOfUs",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "最后的生还者标签图片",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "management",
    "storage",
    "<all_urls>"
  ],
  "chrome_url_overrides":{
    "newtab":"image.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/*"
      ],
      "css": [
        "css/image.css"
      ],
      "js": [
        "js/jquery-1.12.3.min.js",
        "js/image.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'"
}